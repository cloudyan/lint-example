{
  "name": "lint-example",
  "version": "1.0.0",
  "description": "lint example",
  "keywords": [],
  "homepage": "https://github.com/cloudyan/lint-example#readme",
  "bugs": {
    "url": "https://github.com/cloudyan/lint-example/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudyan/lint-example.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "prettier": "prettier -w .",
    "prettier:ci": "prettier -c .",
    "sort": "npx sort-package-json",
    "test": "echo 'test success'",
    "test:fail": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.ts?(x)": [
      "prettier --parser=typescript --write --ignore-unknown"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  }
}
